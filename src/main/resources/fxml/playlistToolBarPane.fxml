<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.ztp.mp3player.controller.PlaylistToolBarPaneController">
   <children>
      <HBox alignment="CENTER" spacing="7.0">
         <children>
          <Button fx:id="addButton" contentDisplay="CENTER" mnemonicParsing="false" text="Add" />
            <Button fx:id="deleteButton" alignment="CENTER" contentDisplay="CENTER" disable="true" mnemonicParsing="false" text="Delete" />
            <Button fx:id="editButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" disable="true" mnemonicParsing="false" text="Edit" />
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
